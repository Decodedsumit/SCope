{
  "name": "scope-client",
  "version": "0.0.7",
  "description": "Large-scale visualization tool for single-cell datasets",
  "main": "main.js",
  "scripts": {
    "preinstall": "npm install ./opt/scopeserver/bindserver --prefix .",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron": "electron .",
    "package-linux-x64": "sudo electron-packager . scope --overwrite --platform=linux --arch=x64 --prune=true --out=release --ignore=assets/scope-client-0.0.[0-5].*.js --ignore=opt/scopeserver/pyvenv --ignore=opt/scopeserver/setup.py --ignore=opt/scopeserver/build --ignore=opt/scopeserver/test --ignore=node_modules --ignore=opt/scopeserver/bindserver/node_modules"
  },
  "keywords": [
    "single-cell",
    "gene",
    "regulatory",
    "networks",
    "large-scale-visualization"
  ],
  "author": "M.D.W",
  "license": "ISC",
